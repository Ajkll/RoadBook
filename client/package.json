{
  "name": "client",
  "main": "expo-router/entry",
  "version": "3.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "start:expo": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --config=jest.config.mjs",
    "lint": "eslint --fix .",
    "test:coverage": "jest --config=jest.config.mjs --coverage",
    "preversion": "node update-app-config.js",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "push-release": "git push --follow-tags origin test-env",
    "full-release": "yarn release && yarn push-release"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/bottom-sheet": "^5.1.2",
    "@miblanchard/react-native-slider": "^2.6.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.3.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "@react-navigation/stack": "^6.3.25",
    "@reduxjs/toolkit": "^2.6.0",
    "@turf/turf": "^7.2.0",
    "@types/react-native": "^0.73.0",
    "ajv": "^8.12.0",
    "axios": "^1.8.4",
    "expo": "~53.0.7",
    "expo-av": "~15.1.4",
    "expo-camera": "~16.1.6",
    "expo-clipboard": "~7.1.4",
    "expo-constants": "^17.1.6",
    "expo-dev-client": "~5.1.8",
    "expo-device": "~7.1.4",
    "expo-dotenv": "^0.1.0",
    "expo-font": "~13.3.1",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.4",
    "expo-location": "~18.1.4",
    "expo-notifications": "~0.31.1",
    "expo-router": "~5.0.5",
    "expo-secure-store": "^14.2.3",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.7",
    "expo-updates": "~0.28.12",
    "expo-web-browser": "~14.1.6",
    "express": "^5.1.0",
    "firebase": "10.11.1",
    "formik": "^2.4.6",
    "http-proxy-middleware": "^3.0.3",
    "loglevel": "^1.9.2",
    "lucide-react-native": "^0.503.0",
    "mapbox-gl": "^3.11.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-actions-sheet": "^0.9.7",
    "react-native-chart-kit": "^6.12.0",
    "react-native-file-logger": "^0.6.0",
    "react-native-gesture-handler": "2.24.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-maps": "1.20.1",
    "react-native-modalize": "^2.1.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-reanimated-carousel": "^4.0.2",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-snap-carousel": "^1.6.1",
    "react-native-svg": "15.11.2",
    "react-native-toast-message": "^2.3.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "^0.20.0",
    "react-navigation": "^5.0.0",
    "react-redux": "^9.2.0",
    "sentry-expo": "^7.2.0",
    "swiper": "^11.2.6",
    "yup": "^1.6.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "react"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@eslint/js": "^9.21.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/cli": "^18.0.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.1.0",
    "@types/react": "~19.0.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.22.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.4",
    "jest-summary-reporter": "^0.0.2",
    "prettier": "^3.5.3",
    "react-test-renderer": "^18.3.1",
    "standard-version": "^9.5.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.26.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  },
  "private": true,
  "type": "commonjs",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-chart-kit",
          "react-native-modalize",
          "react-native-snap-carousel"
        ]
      }
    }
  }
}
