{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle",
        "credentialsSource": "remote",
        "gradleCommand": ":app:bundleRelease",
        "withoutCredentials": false
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_DEBUG": "true"
      },
      "cache": {
        "key": "production-v1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}